<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
   
<mapper namespace="com.shop.goodee.item.ItemMapper">

	<insert id="setAdd" parameterType="ItemVO">
		INSERT INTO ITEM VALUES(NULL, #{id}, #{title}, #{contents}, #{price}, #{stock}, #{count}, #{url}, SYSDATE, 0, 0)
	</insert>
	
	<select id="getList" resultType="ItemVO">
		SELECT * FROM ITEM ORDER BY ASC;
	</select>
	
	
</mapper>